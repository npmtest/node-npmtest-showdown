{
    "author": "kai zhu <kaizhu256@gmail.com>",
    "description": "#### test coverage for  [showdown (v1.6.4)](http://showdownjs.github.io/showdown/)  [![npm package](https://img.shields.io/npm/v/npmtest-showdown.svg?style=flat-square)](https://www.npmjs.org/package/npmtest-showdown) [![travis-ci.org build-status](https://api.travis-ci.org/npmtest/node-npmtest-showdown.svg)](https://travis-ci.org/npmtest/node-npmtest-showdown)",
    "devDependencies": {
        "electron-lite": "kaizhu256/node-electron-lite#alpha",
        "showdown": "*",
        "utility2": "kaizhu256/node-utility2#alpha"
    },
    "engines": {
        "node": ">=4.0"
    },
    "homepage": "https://github.com/npmtest/node-npmtest-showdown",
    "keywords": [
        "coverage",
        "test",
        "showdown"
    ],
    "license": "MIT",
    "main": "lib.npmtest_showdown.js",
    "name": "npmtest-showdown",
    "nameAlias": "npmtest_showdown",
    "nameOriginal": "npmtest-showdown",
    "os": [
        "darwin",
        "linux"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/npmtest/node-npmtest-showdown.git"
    },
    "scripts": {
        "build-ci": "utility2 shReadmeTest build_ci.sh",
        "env": "env",
        "heroku-postbuild": "(set -e; npm install \"kaizhu256/node-utility2#alpha\"; utility2 shDeployHeroku)",
        "postinstall": "if [ -f npm_scripts.sh ]; then ./npm_scripts.sh postinstall; fi",
        "start": "(set -e; export PORT=${PORT:-8080}; utility2 start test.js)",
        "test": "(set -e; export PORT=$(utility2 shServerPortRandom); utility2 test test.js)"
    },
    "version": "0.0.2"
}
